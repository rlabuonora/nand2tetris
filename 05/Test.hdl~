/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP Test {
    IN a[16];
    OUT out;

    PARTS:
    // Put your code here:
    And(a=[0], b=true, out=out);
}
